public int main(){
    putnum(new c1().m1(4, 9.5));
    return 0;
}

public class c1 {
    int x = 15;
    public int m1(int a, int b){
        int c;
        int count = 0;
        c = true;
        a = b / 2;
        while(c){
            if((a / 2) * 2 != a){
                a = a * 2;
            } else {
                a = a - 1;
            }
            c = a < this.x;
            putnum(a);
            count = count + 1;
        }
        return count;
    }
}